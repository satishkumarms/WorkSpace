<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>MyWebDemos</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>HelloServlet</servlet-name>
    <servlet-class>com.servlets.HelloServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloServlet</servlet-name>
    <url-pattern>/hs</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ServletSession</servlet-name>
    <servlet-class>com.servlets.ServletSession</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletSession</servlet-name>
    <url-pattern>/ss</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ServletDemo</servlet-name>
    <servlet-class>com.servlets.ServletDemo</servlet-class>
    <init-param>
      <param-name>Greetings</param-name>
      <param-value>Hello</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletDemo</servlet-name>
    <url-pattern>/sd</url-pattern>
  </servlet-mapping>
  <context-param>
    <param-name>dbStringUrl</param-name>
    <param-value>jdbc:oracle:thin:@localhost:1521:XE</param-value>
  </context-param>
  <context-param>
    <param-name>dbDriver</param-name>
    <param-value>oracle.jdbc.driver.OracleDriver</param-value>
  </context-param>
  <context-param>
    <param-name>username1</param-name>
    <param-value>satishkumar</param-value>
  </context-param>
  <context-param>
    <param-name>password1</param-name>
    <param-value>satishkumarms</param-value>
  </context-param>
</web-app>
<session-config> dada </session-config>